{
  "name": "testportal-gemini",
  "version": "1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*.testportal.pl/*",
        "*://*.testportal.net/*",
        "*://*.teams.microsoft.com/*"
      ],
      "js": ["content.js"]
    }
  ],

  "web_accessible_resources": ["pageScript.js", "timeScript.js", "ai.js"],
  "browser_action": {
    "default_title": "Testportal Anihilator",
    "default_popup": "popup/index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "128": "assets/icon.png"
  }
}
